{
  "name": "how-much",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docs:types": "rimraf public/docs/typedoc && typedoc --options docs/config/typedoc.json",
    "docs:components": "rimraf public/docs/components && vue-docgen -c docs/config/docgen.config.cjs && node ./docs/scripts/build-components-index.mjs",
    "docs:api": "node ./docs/scripts/build-docs-assets.mjs",
    "docs": "concurrently \"npm:docs:types\" \"npm:docs:components\" \"npm:docs:api\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@pinia/nuxt": "^0.5.1",
    "aws-sdk": "2.1311.0",
    "cropperjs": "^1.6.2",
    "fluent-ffmpeg": "^2.1.3",
    "formidable": "^3.5.2",
    "lodash": "^4.17.21",
    "nuxt": "^3.11.2",
    "pinia": "^2.2.4",
    "sass": "^1.77.2",
    "vee-validate": "^4.13.1",
    "video.js": "^8.19.1",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "vue3-toastify": "^0.2.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/formidable": "^3.4.5",
    "@types/lodash": "^4.17.12",
    "autoprefixer": "^10.4.20",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.47",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.14",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-docgen-cli": "^4.79.0",
    "vuetify": "^3.6.6"
  }
}